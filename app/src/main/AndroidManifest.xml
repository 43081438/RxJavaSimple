<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.rxjavademo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
                    android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.RxJavaExplainFirst" android:screenOrientation="portrait"/>
        <activity android:name=".ui.RxJavaExplainSecond" android:screenOrientation="portrait"/>
        <activity android:name=".ui.RxJavaExplainThird" android:screenOrientation="portrait"/>
        <activity android:name=".ui.RxJavaExplainFouth" android:screenOrientation="portrait"/>
    </application>

</manifest>